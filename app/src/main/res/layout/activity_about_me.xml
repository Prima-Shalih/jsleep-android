<?xml version="1.0" encoding="utf-8"?>
        <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFFABB"
            tools:context=".AboutMeActivity">

            <TextView
                android:id="@+id/balanceOfCustomer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="40dp"
                android:text="0.0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/emailOfCustomer" />

            <TextView
                android:id="@+id/emailOfCustomer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="40dp"
                android:text="primashalih@gmail.com"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameOfCustomer" />

            <TextView
                android:id="@+id/nameOfCustomer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="44dp"
                android:layout_marginEnd="40dp"
                android:text="Prima"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/accountDetails" />

            <TextView
                android:id="@+id/accountDetails"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="72dp"
                android:text="Account Details"
                android:textSize="34sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/balanceText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginTop="12dp"
                android:text="Balance"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/emailText" />

            <TextView
                android:id="@+id/emailText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginTop="12dp"
                android:text="Email"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameText" />

            <TextView
                android:id="@+id/nameText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginTop="44dp"
                android:text="Name"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/accountDetails" />

    <EditText
        android:id="@+id/plain_text_input_amount"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="192dp"
        android:hint="Amount"
        android:inputType="text"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accountDetails" />

            <Button
                android:id="@+id/topUpButton"
                android:layout_width="102dp"
                android:layout_height="53dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="180dp"
                android:text="TOP UP"
                android:backgroundTint="#1E2D52"
                android:textColor="#FFFABB"
                app:layout_constraintStart_toEndOf="@+id/plain_text_input_amount"
                app:layout_constraintTop_toBottomOf="@+id/accountDetails" />

            <Button
                android:id="@+id/registerRenterButton"
                android:layout_width="218dp"
                android:layout_height="52dp"
                android:layout_marginStart="104dp"
                android:layout_marginTop="12dp"
                android:text="Register Renter"
                android:backgroundTint="#1E2D52"
                android:textColor="#FFFABB"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/topUpButton" />

            <androidx.cardview.widget.CardView
                android:id="@+id/RegisterCardView"
                android:layout_width="339dp"
                android:layout_height="255dp"
                android:layout_marginStart="36dp"
                android:layout_marginTop="24dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/registerRenterButton"
                tools:visibility="visible">

                <EditText
                    android:id="@+id/registerRenterInsertName"
                    android:layout_width="103dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="36dp"
                    android:layout_marginTop="24dp"
                    android:hint="Name"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/RegisterCardView" />

                <EditText
                    android:id="@+id/registerRenterInsertAddress"
                    android:layout_width="103dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="36dp"
                    android:layout_marginTop="72dp"
                    android:hint="Address"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerRenterInsertName" />

                <EditText
                    android:id="@+id/registerRenterInsertPhoneNumber"
                    android:layout_width="103dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="36dp"
                    android:layout_marginTop="120dp"
                    android:hint="Phone Number"
                    android:inputType="text"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerRenterInsertName" />

                <Button
                    android:id="@+id/RegisterRenterRegisterButton"
                    android:layout_width="128dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="36dp"
                    android:layout_marginTop="180dp"
                    android:text="Register"
                    android:backgroundTint="#1E2D52"
                    android:textColor="#FFFABB"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/registerRenterInsertPhoneNumber"
                    />


                <Button
                    android:id="@+id/RegisterRenterCancelButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cancel"
                    android:backgroundTint="#1E2D52"
                    android:textColor="#FFFABB"
                    android:layout_marginStart="200dp"
                    android:layout_marginTop="180dp"
                    />

            </androidx.cardview.widget.CardView>


    <androidx.cardview.widget.CardView
        android:id="@+id/DataOfRegisterRenter"
        android:layout_width="339dp"
        android:layout_height="255dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registerRenterButton"
        tools:visibility="visible">

        <TextView
            android:id="@+id/NameOfRegisterRenter"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:text="Name"
            android:gravity="left"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DataOfRegisterRenter"
            android:layout_marginStart="36dp"
            android:layout_marginTop="24dp" />

        <TextView
            android:id="@+id/AddressOfRegisterRenter"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="72dp"
            android:inputType="text"
            android:text="Address"
            android:gravity="left"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DataOfRegisterRenter"
            />

        <TextView
            android:id="@+id/PhoneNumberOfRegisterRenter"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="120dp"
            android:inputType="text"
            android:text="Phone Number"
            android:gravity="left"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DataOfRegisterRenter" />

        <TextView
            android:id="@+id/DataOfCustomerNameOfRegisterRenter"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="24dp"
            android:gravity="right"
            android:text="Netlab Store"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DataOfRegisterRenter" />

        <TextView
            android:id="@+id/DataOfAddressOfRegisterRenter"
            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="220dp"
            android:layout_marginTop="72dp"
            android:gravity="right"
            android:text="Gedung DTE Lantai 2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DataOfCustomerNameOfRegisterRenter" />

        <TextView
            android:id="@+id/DataOfPhoneNumberOfRegisterRenter"
            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="220dp"
            android:layout_marginTop="120dp"
            android:gravity="right"
            android:text="0123"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DataOfAddressOfRegisterRenter" />

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/LogOutButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="40dp"
        android:clickable="true"
        android:text="Log Out"
        android:textColor="#1E2D52"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/balanceOfCustomer" />

</androidx.constraintlayout.widget.ConstraintLayout>